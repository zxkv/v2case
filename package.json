			"eslint --fix"
		],
		"*.{html,css,less,scss,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"axios": "^1.6.2",
		"better-mock": "^0.3.7",
		"dayjs": "^1.11.10",
		"element-ui": "^2.15.14",
