{
	"name": "vite-v2case",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"author": "zxkvv@qq.com",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint 'src/**/*.{js,jsx,vue}' --fix",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,tsx,vue}": [
