		"nprogress": "^0.2.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persistedstate": "^3.2.1",
		"qs": "^6.11.2",
		"vue": "~2.7.15",
		"vue-router": "~3.6.5"
	},
	"devDependencies": {
		"@vitejs/plugin-vue2": "^2.3.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-vue": "^9.19.2",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
